<?xml version="1.0" encoding="utf-8"?>
<xs:scheme xmlns:xs="http://www.w3.org/2001/XMLSchema">
    <xs:element name="Faculty">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="specialization">
                    <xs:complexType>
                        <xs:sequence>
                            <xs:element name="name" type="xs:string"/>
                            <xs:element name="mark" type="xs:string"/>
                            <xs:element name="plan" type="xs:string"/>
                            <xs:element name="city" type="xs:string"/>
                        </xs:sequence>
                    </xs:complexType>
                </xs:element>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
</xs:schema>